    }
}
#intro {
    width: 70%;
    margin:auto;
    background-color:#fafafa;
    padding:1px;
    -webkit-box-shadow: 0px 0px 14px 2px rgba(0,0,0,0.1);
    -moz-box-shadow: 0px 0px 14px 2px rgba(0,0,0,0.1);
